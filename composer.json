{
    "name": "datahivedevelopment/laravel-introspection",
    "description": "Laravel Introspection - Provides OAuth Introspection capabilities for Laravel Passport",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [{
        "name": "Dustin Schreiber (DataHive Development)",
        "email": "support@datahivedev.com"
    }],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.0",
        "laravel/passport": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "DataHiveDevelopment\\Introspection\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DataHiveDevelopment\\Introspection\\IntrospectionServiceProvider"
            ]
        }
    }
}
